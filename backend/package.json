{
  "name": "backend-armazem",
  "version": "1.0.0",
  "description": "Backend do sistema de armazem",
  "main": "server.js",
  "author": "Murilo Melo",
  "license": "MIT",
  "scripts": {
    "start": "node -r sucrase/register ./src/app.js",
    "dev": "nodemon --exec \"sucrase-node src/app.js\"",
    "test": "babel-node --extensions .js,.jsx node_modules/.bin/tap test/**/*.test.js",
    "lint": "eslint . --ext .js",
    "build": "sucrase src -d ./dist --transforms imports",
    "format": "prettier --write ."
  },
  "jest": {
    "testEnvironment": "node",
    "transform": {},
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ]
  },
  "dependencies": {
    "@babel/core": "^7.24.3",
    "@babel/plugin-transform-modules-commonjs": "^7.24.1",
    "@fastify/cors": "8.0.0",
    "@prisma/client": "^5.11.0",
    "axios": "^1.6.7",
    "babel-jest": "^29.7.0",
    "bcrypt": "^5.1.1",
    "date-fns": "^3.3.1",
    "dotenv": "^16.4.5",
    "eslint": "^8.13.0",
    "eslint-plugin-import-helpers": "^1.3.1",
    "fastify": "^4.26.2",
    "fs": "^0.0.1-security",
    "jest": "^29.7.0",
    "jsonwebtoken": "^9.0.2",
    "nodemon": "^3.1.0",
    "path": "^0.12.7",
    "pino-pretty": "^11.0.0",
    "prisma": "^5.11.0",
    "supertest": "^6.3.4",
    "tap": "^18.7.2",
    "winston": "^3.12.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.24.3",
    "@mermaid-js/mermaid-cli": "^10.8.0",
    "eslint": "^8.57.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-semistandard": "^17.0.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.0.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-promise": "^6.0.0",
    "prettier": "^3.2.5",
    "prisma-dbml-generator": "^0.12.0",
    "prisma-erd-generator": "^1.11.2",
    "sucrase": "^3.35.0"
  }
}
